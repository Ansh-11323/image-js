{
  "plugins": [
    "transform-es2015-block-scoping"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "npm": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": 6
            },
            "exclude": [
              "transform-es2015-for-of"
            ]
          }
        ]
      ]
    },
    "browser": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "chrome >= 54",
                "firefox >= 45",
                "last 2 edge versions",
                "last 1 safari version"
              ]
            },
            "exclude": [
              "transform-es2015-for-of"
            ]
          }
        ]
      ]
    }
  }
}
