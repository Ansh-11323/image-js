{
  "plugins": [
    "transform-es2015-block-scoping"
  ],
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": 6
            },
            "exclude": [
              "transform-es2015-for-of"
            ]
          }
        ]
      ]
    },
    "browser": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "last 2 chrome versions",
                "last 2 edge versions",
                "last 1 safari version"
              ]
            },
            "exclude": [
              "transform-es2015-for-of"
            ]
          }
        ]
      ]
    }
  }
}
