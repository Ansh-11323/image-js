    "array.prototype.flat": "^1.2.3",
    "eslint": "^6.8.0",
    "eslint-config-cheminfo": "^2.0.4",
    "eslint-config-cheminfo-typescript": "^4.1.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "fs-extra": "^8.1.0",
    "jest": "^25.1.0",
